				И РАЗНОСТЬДАТ(&amp;ТекущаяДата, ТоварыНаСкладахОстатки.СерияНоменклатуры.СрокГодности, ДЕНЬ) &lt; 85
			ТОГДА ТоварыНаСкладахОстатки.КоличествоОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК Количество85,
	ВЫБОР
		КОГДА РАЗНОСТЬДАТ(&amp;ТекущаяДата, ТоварыНаСкладахОстатки.СерияНоменклатуры.СрокГодности, ДЕНЬ) &gt;= 85
				И РАЗНОСТЬДАТ(&amp;ТекущаяДата, ТоварыНаСкладахОстатки.СерияНоменклатуры.СрокГодности, ДЕНЬ) &lt; 100
			ТОГДА ТоварыНаСкладахОстатки.КоличествоОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК Количество100,
	ВЫБОР
		КОГДА ТоварыНаСкладахОстатки.СерияНоменклатуры.СрокГодности = ДАТАВРЕМЯ(1, 1, 1)
				ИЛИ ТоварыНаСкладахОстатки.СерияНоменклатуры.абс_ДатаПроизводства = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА РАЗНОСТЬДАТ(ТоварыНаСкладахОстатки.СерияНоменклатуры.абс_ДатаПроизводства, ТоварыНаСкладахОстатки.СерияНоменклатуры.СрокГодности, ДЕНЬ) = 0
					ТОГДА 0
				ИНАЧЕ ВЫРАЗИТЬ(100 * РАЗНОСТЬДАТ(&amp;ТекущаяДата, ТоварыНаСкладахОстатки.СерияНоменклатуры.СрокГодности, ДЕНЬ) / РАЗНОСТЬДАТ(ТоварыНаСкладахОстатки.СерияНоменклатуры.абс_ДатаПроизводства, ТоварыНаСкладахОстатки.СерияНоменклатуры.СрокГодности, ДЕНЬ) КАК ЧИСЛО(15, 2))
			КОНЕЦ
	КОНЕЦ КАК ПроцентСвежести
ПОМЕСТИТЬ ТЗ
ИЗ
	РегистрНакопления.ТоварыНаСкладах.Остатки(
			&amp;ТекущаяДата,
			Склад В ИЕРАРХИИ (&amp;Склад)
				И Номенклатура.ВестиУчетПоСериям = ИСТИНА) КАК ТоварыНаСкладахОстатки
;

