    <edit>
      <common>true</common>
    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВложенныйЗапрос.ЗаказПокупателя,
	ВложенныйЗапрос.Контрагент,
	ВложенныйЗапрос.Номер КАК Номер,
	ВложенныйЗапрос.Дата,
	ВложенныйЗапрос.Статус,
	ВложенныйЗапрос.ДатаОплаты,
	ВложенныйЗапрос.ДатаОтгрузки,
	ВложенныйЗапрос.ДоговорКонтрагента,
	ВложенныйЗапрос.Ответственный,
	ВложенныйЗапрос.СтатусОтгрузки,
	ВложенныйЗапрос.абс_СуммаСНДС,
	ВложенныйЗапрос.абс_СуммаБезНДС,
	ВложенныйЗапрос.Валюта,
	ВложенныйЗапрос.абс_ДатаПоставкиВТЦ,
	ВложенныйЗапрос.Грузополучатель,
	ВложенныйЗапрос.СуммаОбщейЗадолженности,
	ВложенныйЗапрос.СуммаПросроченнойЗадолженности,
	ВложенныйЗапрос.КоличествоДнейПросроченнойЗадолженности,
	ВложенныйЗапрос.ДопустимоеЧислоДнейЗадолженности,
	ВложенныйЗапрос.ПроцентПДЗ,
	ВложенныйЗапрос.Организация.Префикс КАК Организация,
	ВложенныйЗапрос.ДатаАктуальности,
	ВложенныйЗапрос.СтатусДебитора,
	ВложенныйЗапрос.МенеджерПокупателя
ИЗ
	(ВЫБРАТЬ
		Заказы.Ссылка КАК ЗаказПокупателя,
		Заказы.Контрагент КАК Контрагент,
		Заказы.Номер КАК Номер,
		Заказы.Дата КАК Дата,
		ЕСТЬNULL(абс_КонтрольДЗПоЗаказамСрезПоследних.СтатусДЗ, ЗНАЧЕНИЕ(Перечисление.абс_СтатусыДЗПоЗаказам.Блокирован)) КАК Статус,
		ВЫБОР
			КОГДА Заказы.ДатаОтгрузки = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА ДОБАВИТЬКДАТЕ(Заказы.Дата, ДЕНЬ, ЕСТЬNULL(Заказы.ДоговорКонтрагента.ДопустимоеЧислоДнейЗадолженности, 0))
			ИНАЧЕ ДОБАВИТЬКДАТЕ(Заказы.ДатаОтгрузки, ДЕНЬ, ЕСТЬNULL(Заказы.ДоговорКонтрагента.ДопустимоеЧислоДнейЗадолженности, 0))
		КОНЕЦ КАК ДатаОплаты,
		Заказы.ДатаОтгрузки КАК ДатаОтгрузки,
		Заказы.ДоговорКонтрагента КАК ДоговорКонтрагента,
		Заказы.Организация КАК Организация,
		Заказы.Ответственный КАК Ответственный,
		Заказы.абс_Статус КАК СтатусОтгрузки,
		Заказы.абс_СуммаСНДС КАК абс_СуммаСНДС,
		Заказы.абс_СуммаБезНДС КАК абс_СуммаБезНДС,
		Заказы.абс_ДатаПоставкиВТЦ КАК абс_ДатаПоставкиВТЦ,
		Заказы.Грузополучатель КАК Грузополучатель,
		абс_СтопЛистПоКонтрагентамСрезПоследних.СуммаОбщейЗадолженности КАК СуммаОбщейЗадолженности,
		абс_СтопЛистПоКонтрагентамСрезПоследних.СуммаПросроченнойЗадолженности КАК СуммаПросроченнойЗадолженности,
		абс_СтопЛистПоКонтрагентамСрезПоследних.КоличествоДнейПросроченнойЗадолженности КАК КоличествоДнейПросроченнойЗадолженности,
		абс_СтопЛистПоКонтрагентамСрезПоследних.ДопустимоеЧислоДнейЗадолженности КАК ДопустимоеЧислоДнейЗадолженности,
		ВЫБОР
			КОГДА ЕСТЬNULL(абс_СтопЛистПоКонтрагентамСрезПоследних.СуммаОбщейЗадолженности, 0) = 0
				ТОГДА 0
			ИНАЧЕ ВЫРАЗИТЬ(ЕСТЬNULL(абс_СтопЛистПоКонтрагентамСрезПоследних.СуммаПросроченнойЗадолженности, 0) / абс_СтопЛистПоКонтрагентамСрезПоследних.СуммаОбщейЗадолженности * 100 КАК ЧИСЛО(15, 2))
		КОНЕЦ КАК ПроцентПДЗ,
		абс_СтопЛистПоКонтрагентамСрезПоследних.Период КАК ДатаАктуальности,
		Заказы.ВалютаДокумента КАК Валюта,
		СататусДебитора.Значение КАК СтатусДебитора,
		КонтрагентыМенеджерыПокупателя.МенеджерПокупателя КАК МенеджерПокупателя
	ИЗ
		РегистрСведений.абс_СтопЛистПоКонтрагентам.СрезПоследних КАК абс_СтопЛистПоКонтрагентамСрезПоследних
			ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказПокупателя КАК Заказы
				ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.абс_КонтрольДЗПоЗаказам.СрезПоследних КАК абс_КонтрольДЗПоЗаказамСрезПоследних
				ПО Заказы.Ссылка = абс_КонтрольДЗПоЗаказамСрезПоследних.ЗаказПокупателя
				ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.МенеджерыПокупателя КАК КонтрагентыМенеджерыПокупателя
				ПО Заказы.Контрагент = КонтрагентыМенеджерыПокупателя.Ссылка
				ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
					ЗначенияСвойствОбъектов.Значение КАК Значение,
					ЗначенияСвойствОбъектов.Объект КАК Объект
				ИЗ
					РегистрСведений.ЗначенияСвойствОбъектов КАК ЗначенияСвойствОбъектов
				ГДЕ
					ЗначенияСвойствОбъектов.Свойство = ЗНАЧЕНИЕ(ПланВидовХарактеристик.СвойстваОбъектов.лео_СтатусДебитора)) КАК СататусДебитора
				ПО Заказы.Контрагент = СататусДебитора.Объект
			ПО абс_СтопЛистПоКонтрагентамСрезПоследних.Контрагент = Заказы.Контрагент
	ГДЕ
		НЕ абс_СтопЛистПоКонтрагентамСрезПоследних.СтатусЗадолженности = ЗНАЧЕНИЕ(Перечисление.абс_СтатусыЗадолженности.Разблокирован)
		И НЕ абс_СтопЛистПоКонтрагентамСрезПоследних.СтатусЗадолженности = ЗНАЧЕНИЕ(Перечисление.абс_СтатусыЗадолженности.ПустаяСсылка)
		И (Заказы.абс_Статус = ЗНАЧЕНИЕ(Перечисление.абс_СтатусыОтгрузки.Создан)
				ИЛИ Заказы.абс_Статус = ЗНАЧЕНИЕ(Перечисление.абс_СтатусыОтгрузки.Согласован)
				ИЛИ Заказы.абс_Статус = ЗНАЧЕНИЕ(Перечисление.абс_СтатусыОтгрузки.НеСогласован)
				ИЛИ Заказы.абс_Статус = ЗНАЧЕНИЕ(Перечисление.абс_СтатусыОтгрузки.КОтгрузке))
		И Заказы.Дата >= ДАТАВРЕМЯ(2014, 6, 1)) КАК ВложенныйЗапрос
ГДЕ
	НЕ ВложенныйЗапрос.Статус = ЗНАЧЕНИЕ(Перечисление.абс_СтатусыДЗПоЗаказам.Разблокирован)

УПОРЯДОЧИТЬ ПО
	Номер</queryText>
