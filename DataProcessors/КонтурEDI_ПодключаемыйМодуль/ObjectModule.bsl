	НоваяСтрока.ТипУпаковки		 = "201";
	НоваяСтрока.ДопИдентификатор = ТекUltimateCustomerGln;
		
	ДМ_ПронумероватьУпаковкиСквознымПорядковымНомером(ФормаСообщения); 
	ДМ_ПроименоватьУпаковки(ФормаСообщения);
	
	
