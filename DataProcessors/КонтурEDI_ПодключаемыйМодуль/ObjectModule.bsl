				НомУпак = "02" + GTINНоменклатуры + "37" + СтрЗаменить(Строка(Цел(КоличествоКодовЧЗ)),Символы.НПП,""); 
		
				СписокКодовМаркировки = Новый СписокЗначений;
				СписокКодовМаркировки.Добавить(НомУпак, "Групповой"); // Group - уникальный идентификатор вторичной (потребительской)/третичной (заводской, транспортной) упаковки [НомУпак].
				//СписокКодовМаркировки.Добавить(НомУпак, "Индивидуальный"); // Units — код маркировки отдельной единицы (потребительской упаковки)[КИЗ]
				//СписокКодовМаркировки.Добавить(НомУпак, "Транспортный"); // TransPackageId — уникальный идентификатор транспортной упаковки [ИдентТрансУпак]
				//СписокКодовМаркировки.Добавить(НомУпак, "Набор"); // Mix
				
				Строка.КодыМаркировки = СписокКодовМаркировки;
				//Владимир пока комментируем до выяснения
				//Строка.ТипКодовМаркировки = "Group";		// Group - уникальный идентификатор вторичной (потребительской)/третичной (заводской, транспортной) упаковки [НомУпак].
				//Строка.ТипКодовМаркировки = "Transport";	// TransPackageId — уникальный идентификатор транспортной упаковки [ИдентТрансУпак]
				//Строка.ТипКодовМаркировки = "Unit";		// Units — код маркировки отдельной единицы (потребительской упаковки)[КИЗ]
				//Строка.ТипКодовМаркировки = "Mix";		// Mix
			КонецЕсли;	
		КонецЕсли;	
	КонецЕсли;	
