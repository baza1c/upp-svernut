Процедура ДобавитьКодыМаркировкиТоваров(Строка, ДокументРеализация, Покупатель1С) // *** Владимир
	//Проверим необходимость простановки ОСУ
    Если ДокументРеализация.Дата>=Дата(2025,9,1,0,0,0) ИЛИ лео_ДопФункции.Получить_ЗначенияКатегорииОбъектов(ДокументРеализация.Контрагент, Справочники.КатегорииОбъектов.НайтиПоКоду("000000128")) Тогда
		ЗначениеОКПД2	= лео_ДопФункции.Получить_ЗначенияСвойствОбъектов(Строка.Номенклатура, ПланыВидовХарактеристик.СвойстваОбъектов.НайтиПоКоду("00000000066"));
		Если ЗначениеЗаполнено(ЗначениеОКПД2) И ТипЗнч(ЗначениеОКПД2)=Тип("СправочникСсылка.Лео_КлассификаторОКПД2") Тогда 
			Если ЗначениеОКПД2.ЧестныйЗнак Тогда
				Если ДокументРеализация.СписокКоробок.Количество()>0 Тогда   //заполнена       ИЛИ ДокументРеализация.ШтучныеQR_Коды.Количество()>0
					Отбор	=	Новый  Структура;
					Отбор.Вставить("Номенклатура",Строка.Номенклатура); 
					РезультатОтбора	= 	ДокументРеализация.СписокКоробок.НайтиСтроки(Отбор);
					СписокКодовМаркировки = Новый СписокЗначений;
					Для Каждого СтрокаКИЗ Из РезультатОтбора Цикл
						СписокКодовМаркировки.Добавить(СтрокаКИЗ.ШтрихКодКоробки.Наименование, "Транспортный");
					КонецЦикла;
					Строка.КодыМаркировки = СписокКодовМаркировки;
				КонецЕсли;	
				Если ДокументРеализация.ШтучныеQR_Коды.Количество()>0 Тогда    
					Отбор	=	Новый  Структура;
					Отбор.Вставить("Номенклатура",Строка.Номенклатура); 
					РезультатОтбора	= 	ДокументРеализация.ШтучныеQR_Коды.НайтиСтроки(Отбор);
					СписокКодовМаркировки = Новый СписокЗначений;
					Для Каждого СтрокаКИЗ Из РезультатОтбора Цикл
						СписокКодовМаркировки.Добавить(Сред(СокрЛП(СтрокаКИЗ.QR_Код_Входящий),1,31), "Индивидуальный");
					КонецЦикла;
					Строка.КодыМаркировки = СписокКодовМаркировки;
				КонецЕсли;
				Если ДокументРеализация.СписокКоробок.Количество()=0 И ДокументРеализация.ШтучныеQR_Коды.Количество()=0 Тогда
					Сообщить("В документе "+ ДокументРеализация.Ссылка +"есть товар БАД, но коды не заполнену");	
					GTINНоменклатуры = лео_ДопФункции.ПолучитьGTINпоНоменклатуре();
					КоличествоКодовЧЗ = Строка.Количество;
					НомУпак = "02" + GTINНоменклатуры + "37" + СтрЗаменить(Строка(Цел(КоличествоКодовЧЗ)),Символы.НПП,""); 
					СписокКодовМаркировки = Новый СписокЗначений;
					СписокКодовМаркировки.Добавить(НомУпак, "Групповой"); 
					Строка.КодыМаркировки = СписокКодовМаркировки;
