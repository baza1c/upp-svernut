Функция Плагин_ПечатьПалетногоЛиста(ИмяСобытия, Параметры, СтандартнаяОбработкаEDI)
		
	Если ИмяСобытия = "ПриОткрытииФормыСообщения" Тогда
				
		//добавить форму сообщения DESADV кнопку "Печать паллетных листов" (SSCC)
		Попытка
			Если Параметры.Сообщение.ТипСообщения = "DESADV"
				 И Параметры.Сообщение.Свойство("Транспортныеупаковки") Тогда
				 //И ЗначениеЗаполнено(Параметры.Сообщение.СообщениеСсылка) 
				#Если Клиент Тогда 
					КнопкиКоманднойПанели = Параметры.ФормаСообщения.НастройкаФормы.КнопкиКоманднойПанели;
					НоваяСтрока = КнопкиКоманднойПанели.Добавить();
					НоваяСтрока.ВидКнопки 					= "Кнопка";
					НоваяСтрока.Имя 						= "ПечатьПалетныхЛистов";
					НоваяСтрока.ИсточникКартинки			= "ЭлементФормы";
					НоваяСтрока.КнопкаПоУмолчанию 			= Ложь;
					НоваяСтрока.Представление				= "Печать палетных листов";
					НоваяСтрока.ПроверятьПоля 				= Ложь;
					НоваяСтрока.СохранятьСоответствия 		= Ложь;
					ДействиеКнопки 			= Новый Действие("КнопкаДействияФормыНажатие");
					КнопкиСписка 			= Параметры.ФормаСообщения.ЭлементыФормы.ОсновныеДействияФормы.Кнопки;
					НоваяКнопка 			= КнопкиСписка.Добавить("ПечатьПалетныхЛистов", ТипКнопкиКоманднойПанели.Действие, "Печать палетных листов", ДействиеКнопки);
					НоваяКнопка.Картинка 	= БиблиотекаКартинок.Печать;
					НоваяКнопка.Отображение = ОтображениеКнопкиКоманднойПанели.НадписьКартинка;
					КоличествоКнопок 		= КнопкиСписка.Количество();
					КнопкиСписка.Сдвинуть(КнопкиСписка[КоличествоКнопок-1],-КоличествоКнопок+1);
				#Иначе
					КомандаПечатьПалетныхЛистов 			= Параметры.ФормаСообщения.Команды.Добавить("КомандаПечатьПалетныхЛистов");
					КомандаПечатьПалетныхЛистов.Действие 	= "КнопкаДополнительныеДействияФормыНажатие";
					КомандаПечатьПалетныхЛистов.Заголовок 	= "Печать палетных листов";
					КнопкаПечатьПалетныхЛистов 				= Параметры.ФормаСообщения.Элементы.Добавить("КнопкаПечатьПалетныхЛистов",Тип("КнопкаФормы"),Параметры.ФормаСообщения.Элементы.ФормаКоманднаяПанель);
					КнопкаПечатьПалетныхЛистов.Вид 			= ВидКнопкиФормы.ОбычнаяКнопка;
					КнопкаПечатьПалетныхЛистов.ИмяКоманды 	= "КомандаПечатьПалетныхЛистов";
					КнопкаПечатьПалетныхЛистов.Картинка 	= БиблиотекаКартинок.Печать;
					КнопкаПечатьПалетныхЛистов.Отображение 	= ОтображениеКнопки.КартинкаИТекст;
					ОсновнойМодуль.ДобавитьКнопкуФормыСообщения(Параметры.ФормаСообщения.НастройкаФормы.КнопкиКоманднойПанели,"КнопкаФормы",	"КомандаПечатьПалетныхЛистов", "Печать палетных листов",	Ложь,	,"ЭлементФормы", БиблиотекаКартинок.Печать);
				#КонецЕсли
 			КонецЕсли;
		Исключение
			_ОписаниеОшибки = ОписаниеОшибки();
			Сообщить("Ошибка: " + _ОписаниеОшибки);
		КонецПопытки;
		
	КонецЕсли;
	
	Если ИмяСобытия = "КнопкаФормыСообщенияНажатие" Тогда
		
		//алгоритм печати палетных листов из формы сообщения DESADV (SSCC)
		//благодарность за помощь в разработке программисту ООО "Вознесенский пищевой комбинат"
		
		Если Параметры.Свойство("ИмяКнопки")  и Параметры.ИмяКнопки  = "ПечатьПалетныхЛистов"  		 или //для ОФ
			 Параметры.Свойство("ИмяКоманды") и Параметры.ИмяКоманды = "КомандаПечатьПалетныхЛистов" Тогда  //для УФ
			 
			#Если Не Клиент Тогда
			СтандартнаяОбработкаEDI = Ложь;
			#КонецЕсли
			
			МассивDESADV = Новый Массив;
			Если ЗначениеЗаполнено(Параметры.Сообщение.СообщениеСсылка) Тогда
				МассивDESADV.Добавить(Параметры.Сообщение.СообщениеСсылка);
			КонецЕсли;
			
			Результат = ОсновнойМодуль.ОбработатьМассовуюПечатьПалетныхЛистов(МассивDESADV);
			
			Если Не Результат.Успешно Тогда
				ТекстПредупреждения="Не удалось подключить внешнюю компоненту для формирования картинки штрихкода.";
				ОсновнойМодуль.ВывестиПредупреждение_КонтурEDI(ТекстПредупреждения);
				Возврат Неопределено;
			Иначе
				Если Результат.КоличествоЛистов = 0 Тогда
					ТекстПредупреждения="В сообщении не заполнены данные о транспортных упаковках.";
					ОсновнойМодуль.ВывестиПредупреждение_КонтурEDI(ТекстПредупреждения);
					Возврат Неопределено;
				Иначе
					ТабДокумент = Результат.ТабДокОбщий;
					#Если Клиент Тогда
						Результат.ТабДокОбщий.Показать("Палетные листы ("+Результат.КоличествоЛистов+" шт) - "+Формат(ТекущаяДата(),"ДЛФ=Д"));
					#КонецЕсли	
				КонецЕсли;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	
	#Если Клиент Тогда
	Возврат Неопределено;
	#Иначе
	Возврат ТабДокумент;
	#КонецЕсли
	
КонецФункции

